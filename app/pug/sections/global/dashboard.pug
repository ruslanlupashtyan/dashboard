aside.aside-wrapper
  .aside
    .aside__logo.logo.flex.ai-center
      include ../../../images/svg/logo.svg
      p.logo__title Dashboard
        span v.01
    p.aside__current-page.with-icon(style=`--bg-image:url("../../../images/svg/key-square.svg")`) Dashboard 
    nav.asside__links.links
      ul
        each item, i in nav 
          li
            a.with-icon(href= item.link, class= i===0? "active" : "", style=`--bg-image:url("${item.icon}"); --bg-image-white:url("${item.icon_white}")`)= item.title
    .acide__account.account.flex.ai-center
      .account__image 
        img(src="../../../images/Ellipse 8.png", alt="")
      .account__text 
        p Evano
        p Project Manager
main.main
  .main__title 
    //- (ðŸ‘‹)
    p Hello Evano &#128075;, 
  .main__frame.frame 
    .frame__head.frame-head.flex-sm.jc-between.ia-center 
      .frame-head__text 
        p All customers
        p Active Members
      .frame-head__search.flex.ai-center
        include ../../../images/svg/search.svg
        input(type="search", name="", placeholder="Search")
    .table-wrapper
      table.frame__body.table 
        thead 
          tr
            each head in thead
              th= head
        tbody
          each item in tbody
            tr
              td= item.name
              td= item.company
              td= item.phone
              td= item.email
              td= item.country
              if item.status
                td.active
                  .status Active
              else 
                td.unactive
                  .status Inactive
    .main__footer.flex-md.jc-between 
      p Showing data 1 to 8 of  256K entries
      ul.switch-buttons.flex.ai-center.jc-center
